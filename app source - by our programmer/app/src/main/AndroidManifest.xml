            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version"/>

        <activity android:name=".FindRoom">
        </activity>
        <activity android:name=".Night">
        </activity>
        <activity android:name=".Day">
        </activity>
    </application>

</manifest>